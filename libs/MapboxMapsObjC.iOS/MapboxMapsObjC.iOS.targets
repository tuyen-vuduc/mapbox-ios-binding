<?xml version="1.0" encoding="utf-8"?>
<Project>
	<PropertyGroup>
		<_MapboxDownloadToken>$(MAPBOX_DOWNLOADS_TOKEN)</_MapboxDownloadToken>
		<_MapboxVersion>10.11.1</_MapboxVersion>
		<_MapboxMapsObjCAssemblyName>MapboxMapsObjC.iOS, Version=10.11.1, Culture=neutral, PublicKeyToken=null </_MapboxMapsObjCAssemblyName>
		<_MapboxMapsObjCItemsFolder>MapboxMapsObjC-10.11.1</_MapboxMapsObjCItemsFolder>
		<_MapboxMapsObjCSDKBaseFolder>$(XamarinBuildDownloadDir)$(_MapboxMapsObjCItemsFolder)\</_MapboxMapsObjCSDKBaseFolder>
	</PropertyGroup>
  <ItemGroup Condition="('$(OutputType)'!='Library' OR '$(IsAppExtension)'=='True')">
	<XamarinBuildDownload Include="$(_MapboxMapsObjCItemsFolder)">      
		<Url>https://drive.google.com/uc?id=1jvN95K6SjLB405iRFhTXzPX76rAanFVB&amp;export=download</Url>
		<Kind>Zip</Kind>
	</XamarinBuildDownload>
    <NativeReference Include="$(_MapboxMapsObjCSDKBaseFolder)MapboxMapObjC.xcframework">
      <Kind>Framework</Kind>
      <SmartLink>True</SmartLink>
      <ForceLoad>True</ForceLoad>
    </NativeReference>
  </ItemGroup>
</Project>