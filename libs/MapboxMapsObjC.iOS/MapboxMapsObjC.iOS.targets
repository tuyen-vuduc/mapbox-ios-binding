<?xml version="1.0" encoding="utf-8"?>
<Project>
	<PropertyGroup>
		<_MapboxDownloadToken>$(MAPBOX_DOWNLOADS_TOKEN)</_MapboxDownloadToken>
		<_MapboxMapsObjC>11.3.0</_MapboxMapsObjC>
		<_NugetSuffix>.1</_NugetSuffix>
		<_MapboxMapsObjCAssemblyName>MapboxMapsObjC.iOS, Version=$(_MapboxMapsObjC), Culture=neutral, PublicKeyToken=null </_MapboxMapsObjCAssemblyName>
		<_MapboxMapsObjCItemsFolder>MapboxMapsObjC-$(_MapboxMapsObjC)$(_NugetSuffix)</_MapboxMapsObjCItemsFolder>
		<_MapboxMapsObjCSDKBaseFolder>$(XamarinBuildDownloadDir)$(_MapboxMapsObjCItemsFolder)\</_MapboxMapsObjCSDKBaseFolder>
	</PropertyGroup>
  <ItemGroup Condition="('$(OutputType)'!='Library' OR '$(IsAppExtension)'=='True')">
	<XamarinBuildDownload Include="$(_MapboxMapsObjCItemsFolder)">
		<Url>https://drive.google.com/uc?id=1g0iRXiU5pDMnrbxea6P1cMsHXCgv5I-g&amp;export=download</Url>
		<Kind>Zip</Kind>
	</XamarinBuildDownload>
    <NativeReference Include="$(_MapboxMapsObjCSDKBaseFolder)MapboxMapObjC.xcframework">
      <Kind>Framework</Kind>
      <SmartLink>True</SmartLink>
      <ForceLoad>True</ForceLoad>
    </NativeReference>
  </ItemGroup>
</Project>